{
  "cli": {
    "version": ">= 9.2.0"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_PUBLIC_API_URL": "http://172.16.0.11:8000"
      },
      "android": {
        "buildType": "apk",
        "developmentClient": true,
        "distribution": "internal",
        "applicationArchivePath": "android/app/build/outputs/apk/debug/app-debug.apk"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://backendqf.onrender.com"
      },
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "applicationArchivePath": "android/app/build/outputs/apk/release/app-release.apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
